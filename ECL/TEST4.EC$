	JMP L000001
	SIGNATURE 'TEST4'
S000000
	ENTER 1,1,L000002
	PUSHS 'Size of arg is '
	SVC  2
	PUSHI 1
	SVC  1
	SVC  3
	LEAVE
L000002 EQU 0
CLASS_AB
	ENTER 0,1,L000003
	LEAVE
L000003 EQU 0
CONTROL_CLASS
	ENTER 0,1,L000004
	LEAVE
L000004 EQU 0
DTMF_CLASS
	ENTER 0,1,L000005
	LEAVE
L000005 EQU 0
ERROR_CLASS
	ENTER 0,1,L000006
	LEAVE
L000006 EQU 0
INIT_CLASS
	ENTER 0,1,L000007
	JMP L000008
L000008
	SVC  3
	PUSHS 'Size of mytype is '
	SVC  2
	PUSHI 10
	SVC  1
	SVC  3
	PUSHS 'Size of A is '
	SVC  2
	PUSHI -20
	PUSHI 2
	MUL
	PUSHI -4
	ADD
	SVC  1
	SVC  3
	PUSHI 1
	RANGE 2
	PUSHI 10
	MUL
	PUSHI 2
	RANGE 10
	ADD
	PUSHAX 0,6
	CALL S000000
	PUSHA 1,6
	CALL S000000
	LEAVE
L000007 EQU 1
TIMER_CLASS
	ENTER 0,1,L000009
	LEAVE
L000009 EQU 0
L000001
	INIT 1
	ENTER 3,0,20
	PUSHI -1
	PUSHR 0,-3
	PUSHI 1
	CJE TIMER_CLASS
	PUSHR 0,-3
	PUSHI 2
	CJE CONTROL_CLASS
	PUSHR 0,-3
	PUSHI 16706
	CJE CLASS_AB
	PUSHR 0,-3
	PUSHI 3
	CJE ERROR_CLASS
	PUSHR 0,-3
	PUSHI 4
	CJE DTMF_CLASS
	PUSHR 0,-3
	PUSHI 0
	CJE INIT_CLASS
	JMP -1
	END
